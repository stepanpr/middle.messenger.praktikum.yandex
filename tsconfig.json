{
    "compilerOptions": {
	"lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "dist",
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
	"skipLibCheck": true,
    "strictNullChecks": false,
	"noImplicitAny": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"sourceMap": true,
   },
   "include": ["src", "types.d.ts"],
   "exclude": ["node_modules"],
}
